{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["src/**/vendor/**/*.js"],
    "rules": {
        "semi": [2, "always"],
        "no-unused-vars": [2, {"vars": "all"}]
    }
}
